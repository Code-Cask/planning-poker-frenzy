{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run tailwind -- --minify",
    "build:remix": "remix build",
    "deploy": "fly deploy --remote-only",
    "dev": "run-p dev:*",
    "dev:css": "npm run tailwind -- --watch",
    "dev:remix": "remix dev",
    "tailwind": "tailwindcss -o ./app/styles/tailwind.css",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@remix-run/node": "^1.7.5",
    "@remix-run/react": "^1.7.5",
    "@remix-run/serve": "^1.7.5",
    "isbot": "^3.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.5",
    "@remix-run/eslint-config": "^1.7.5",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "eslint": "^8.23.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  }
}
